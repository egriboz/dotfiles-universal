Pry.prompt = [
  proc { |obj, nest_level, _| "[#{RUBY_VERSION}] (#{obj}):#{nest_level} > " },
  proc { |obj, nest_level, _| "[#{RUBY_VERSION}] (#{obj}):#{nest_level} * " }
]

Pry.config.ls.separator = "\n"

begin
  require 'awesome_print'
  AwesomePrint.defaults={:theme=>:solorized}
  AwesomePrint.pry!
rescue LoadError => err
end